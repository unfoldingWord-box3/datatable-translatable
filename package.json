    "webpack": "^4.0.0"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "4.0.0-alpha.61",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
